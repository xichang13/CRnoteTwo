{
    "markdown.extension.toc.levels": "1..2",
    "MarkdownPaste.rules": [

        {
            "regex": "^(?:https?://)?(?:(?:(?:www\\.?)?youtube\\.com(?:/(?:(?:watch\\?.*?v=([^&\\s]+).*)|))?))",
            "options": "g",
            "replace": "[![](https://img.youtube.com/vi/$1/0.jpg)](https://www.youtube.com/watch?v=$1)"
        },
        {
            "regex": "^(https?://.*)",
            "options": "ig",
            "replace": "[]($1)"
        }
    ],
    "markdown.extension.extraLangIds": [],
}